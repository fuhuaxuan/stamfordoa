[AddtionalSQL]
;操作，没有返回的结果，是在数据库中执行，有Insert, Update, Delete命令。从SQL1～SQL20共支持20条
;将数据更新主表中
SQL1=update Prl_Groundh set (fno,farea,Jarea,fmr,floorno,deptsource,OpType,OpDate,OpGid,OpCode,OpName) = (select ##fno##,##farea##,##Jarea##,##fmr##,##floorno##,##deptsource##,'修改',sysdate,__USRGID__,__USRCODE__,__USRNAME__ from dual) where fGid = ##fGid##

;插入操作记录表
SQL2=insert into Prl_Groundh_Rcd (EntGid,Gid,fGid,fNo,OpType,OpDate,OpGid,OpCode,OpName,OpMemo) select __ENTGID__,sys_guid(),##fGid##,##fno##,'修改',sysdate,__USRGID__,__USRCODE__,__USRNAME__,'编号：'||##fno##||';面积：'||##farea##||';建筑面积：'||##Jarea##||';预算：'||##fmr##||';楼层：'||##floorno##||';项目：'||##deptsource## from dual

