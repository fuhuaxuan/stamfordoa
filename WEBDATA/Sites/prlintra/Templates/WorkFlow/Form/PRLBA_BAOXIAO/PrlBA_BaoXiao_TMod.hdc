[Permission]
;判断FlowGid 是否为空
LIMIT1=FlowGid
LIMITDesc1=StringSize[,0]
LIMITRemind1=对不起，您未指定流程信息！请从正确的路径进入

;判断传入FlowGid 是否存在
SQL1=select 1 from WF_Flow where EntGid = __ENTGID__ and FlowGid = ##FlowGid##
Remind1=对不起，您指定的流程申请单没有找到<script>alert('对不起，您指定的流程申请单没有找到');window.close()</script>

[Detail]
;显示，单条数据的查询，可以用于HDT显示，有select命令。从SQL1～SQL20共支持20条
;取出本流程模型信息
SqlForCondition1=select wm.Code ModelCode, wm.Name ModelName,d.Name StepName from wf_Model wm,WF_Task_Define d where wm.EntGid = d.EntGid(+) and wm.EntGid = __ENTGID__ and wm.ModelGid = d.ModelGid(+) and wm.ModelGid = ##ModelGid## and lower(d.code(+)) = replace(lower(##Url##),'save')

;取出发起人的部门代码
SqlForCondition2=select substr(f.FillDeptCode,0,4) ProCode from WF_^^ModelCode^^ f where f.EntGid = __ENTGID__ and f.FlowGid = ##FlowGid##

;取出本流程主表信息
Sql1=select * from WF_^^ModelCode^^ where EntGid = __ENTGID__ and  FlowGid = ##FlowGid##

;取出本流程已审批的审批人的个数，用于判断是否显示审批人
SQL2=select count(*) IsShow from wf_^^ModelCode^^_App where EntGid = __ENTGID__ and  FlowGid = ##FlowGid## and AppDate is not null

;显示本流程附件信息
LoopSQL1=select Attach_Title ItemGid,Attach_Title ItemName from wf_^^ModelCode^^_Attach where EntGid = __ENTGID__ and  FlowGid = ##FlowGid## order by attach_title
LoopHDT1=Templates\WorkFlow\Form\^^ModelCode^^\LoopSql_Option.hdt
LoopVAR1=AttachList

;显示本流程审批人信息
LoopSQL2=select a.*,decode(a.apptype,0,'发起人',10,'部门经理',20,'部门总经理',30,'财务经理',35,'总经理',40,'商场总经理',50,'资产经理',60,'部门总监',80,'财务总监',90,'区域总经理',95,'Deputy CEO',100,'CEO',110,'发起人通知出纳',120,'出纳付款',130,'发起人修改支付信息') apptypetext  from wf_^^ModelCode^^_App a where FlowGid = ##FlowGid## and AppDate is not null order by AppDate
LoopHDT2=Templates\WorkFlow\Form\^^ModelCode^^\Loopsql_AppList.hdt
LoopVAR2=AppList

;显示本流程明细信息
LoopSQL3=select v.LeftFee - v.YesFee + nvl(fd.ApplyFee,0) AcgFee,fd.* from wf_^^ModelCode^^_Dtl fd,v_PRL_ACG_Fee v where fd.EntGid = __ENTGID__ and fd.FlowGid = ##FlowGid## and fd.EntGid = v.EntGid and fd.AcgGid = v.AcgGid and exists (select 1 from wf_^^ModelCode^^ f where f.EntGid = fd.EntGid and f.FlowGid = fd.FlowGid and f.ComGid = v.ComGid and to_char(f.CreateDate,'YYYY') = v.Year) order by fd.AcgCode
LoopHDT3=Templates\WorkFlow\Form\^^ModelCode^^\Loopsql_DtlMod.hdt
LoopVAR3=DtlList

;显示本流程附件信息
LoopSQL5=select Gid ItemGid,Name ItemName from PRL_COMPANY where EntGid = __ENTGID__ order by code
LoopHDT5=Templates\WorkFlow\Form\^^ModelCode^^\LoopSql_Option.hdt
LoopVAR5=ComList

